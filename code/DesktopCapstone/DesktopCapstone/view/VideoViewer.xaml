<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCapstone.view"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="DesktopCapstone.view.VideoViewer"
        mc:Ignorable="d"
        Title="VideoViewer" Height="450" Width="644">
    <Grid HorizontalAlignment="Right" Width="644">
        <ListBox x:Name="lstSources" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,119,528,10" SelectionChanged="lstSources_SelectionChanged"/>


        <Button x:Name="btnAddSource" Content="Add" HorizontalAlignment="Left" Margin="90,94,0,0" VerticalAlignment="Top" Click="btnAddSource_Click"/>
        <ListBox x:Name="lstNotes" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="139,280,44,23"/>
        <Button x:Name="btnReturn" Content="Return" HorizontalAlignment="Left" Margin="23,13,0,0" VerticalAlignment="Top" Click="btnReturn_Click"/>

        <Wpf:WebView2 x:Name="webVideo" HorizontalAlignment="Left" Height="229" Margin="139,13,0,0" VerticalAlignment="Top" Width="461"/>
        <Button x:Name="btnAddNotes" Content="Add" HorizontalAlignment="Left" Margin="574,255,0,0" VerticalAlignment="Top" Click="btnAddNotes_Click"/>

    </Grid>
</Window>
