<Window x:Class="DesktopCapstone.view.VideoViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCapstone.view"
        mc:Ignorable="d"
        Title="VideoViewer" Height="450" Width="644">
    <Grid HorizontalAlignment="Right" Width="644">
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,119,528,10"/>


        <Button x:Name="btnAddSource" Content="+" HorizontalAlignment="Left" Margin="104,94,0,0" VerticalAlignment="Top"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="491,28,42,73"/>
        <Button x:Name="btnReturn" Content="Return" HorizontalAlignment="Left" Margin="23,13,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
