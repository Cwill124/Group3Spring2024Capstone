<Window x:Class="DesktopCapstone.view.ProjectsViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCapstone.view"
        mc:Ignorable="d"
        Title="ProjectsViewer" Height="450" Width="800">
    <Grid>
        <Grid Grid.Row="0" Background="#3498DB" MinWidth="800" Margin="0,0,0,379">
            <Button x:Name="btnReturn" Content="Home" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="24" Background="#00DDDDDD" BorderBrush="#00707070" Foreground="White"
                    Margin="10,10" />
        </Grid>
        <ListBox d:ItemsSource="{Binding Projects}" Margin="64,119,96,24"/>
        <Button x:Name="btnAddProject" Content="Add" HorizontalAlignment="Left" Margin="664,94,0,0" VerticalAlignment="Top" Click="btnAddProject_Click"/>

    </Grid>
</Window>
