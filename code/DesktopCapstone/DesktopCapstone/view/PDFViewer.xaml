<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCapstone.view"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="DesktopCapstone.view.PDFViewer"
        mc:Ignorable="d"
        Title="PDFViewer" Height="450" Width="644">
    <Grid HorizontalAlignment="Right" Width="644">
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,119,528,10"/>

        <Wpf:WebView2 HorizontalAlignment="Left" Height="353" Margin="181,28,0,0" VerticalAlignment="Top" Width="252"/>
        <Button x:Name="btnAddSource" Content="+" HorizontalAlignment="Left" Margin="104,94,0,0" VerticalAlignment="Top" Click="btnAddSource_Click"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="491,28,42,73"/>
        <Button x:Name="btnReturn" Content="Return" HorizontalAlignment="Left" Margin="23,13,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
